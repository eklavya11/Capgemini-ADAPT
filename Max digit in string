import java.util.Scanner;

public class Source{
    // Code here
    public static int getMaxDigit(String abc){
        if(abc==null||abc.isEmpty()){
            return -1;
        }
        else{
        int ans=-1;
        
        for(int i=0;i<abc.length();i++){
            if(Character.isDigit(abc.charAt(i))){
                int a = Character.getNumericValue(abc.charAt(i));
                if(a>ans){
                    ans = a;
                }
            }
        }
        return ans;
        }
    }
    public static void main(String[] args){
        //Code here
    }
}
